{
    "type": "postgres",
    "host": "${RAILWAY_PRIVATE_DOMAIN}",
    "port": 5432,
    "username": "${POSTGRES_USER}",
    "password": "${POSTGRES_PASSWORD}",
    "database": "${PGDATABASE}",
    "synchronize": false,
    "logging": false,
    "entities": [
        "src/**/*entity.ts"
    ],
    "migrations": [
        "src/migration/**/*.ts"
    ],
    "subscribers": [
        "src/subscriber/**/*.ts"
    ],
    "cli": {
        "entitiesDir": "src/",
        "migrationsDir": "src/migration",
        "subscribersDir": "src/subscriber"
    }
}
